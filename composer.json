{
    "name": "sheer/imagerotator",
    "type": "phpbb-extension",
    "description": "Auto correct image orientation on upload",
    "homepage": "https://www.phpbbguru.net/community/viewtopic.php?f=64&t=44140",
    "version": "1.0.5",
    "time": "2018-09-16",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Sheer",
            "homepage": "https://www.phpbbguru.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
      "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Attached Image rotator",
        "soft-require": {
            "phpbb/phpbb": "3.2.*@dev"
        },
        "version-check": {
            "host": "sheer.phpbbguru.net",
            "ssl": true,
            "directory": "/versions/image_rotator",
            "filename": "version.json"
        }
    }
}
